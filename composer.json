{
    "name": "meirtin/crypton",
    "description": "Laravel API Request Response Encryption",
    "keywords": [
        "meirtin",
        "crypton",
        "laravel request encrypt",
        "laravel response encrypt"
    ],
    "homepage": "https://github.com/meirtin/crypton",
    "license": "MIT",
    "authors": [
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/console": "^7.0|^8.0",
        "illuminate/encryption": "^7.0|^8.0",
        "illuminate/http": "^7.0|^8.0",
        "illuminate/support": "^7.0|^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "laravel/legacy-factories": "^1.0",
        "orchestra/testbench": "^5.0|^6.0",
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Meirtin\\Crypton\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Meirtin\\Crypton\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Meirtin\\Crypton\\CryptonServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
